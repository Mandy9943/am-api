{
  "kind": "collectionType",
  "collectionName": "sellers",
  "info": {
    "singularName": "seller",
    "pluralName": "sellers",
    "displayName": "seller",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "seller"
    },
    "description": {
      "type": "text"
    },
    "telegram": {
      "type": "string"
    },
    "whatsapp": {
      "type": "string",
      "required": true
    },
    "web": {
      "type": "string"
    }
  }
}
